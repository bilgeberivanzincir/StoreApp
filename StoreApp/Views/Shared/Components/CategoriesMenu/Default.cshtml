@model IEnumerable<Category>

<div class="mt3">
    <h6 class="lead">
        <i class="fa fa-solid fa-bars text-secondary"></i>
            Categories
    </h6>
</div>
<div >
    <ul class="list-group">
        @foreach(var cat in Model)
        {
            <li class="list-group-item">
               <a asp-action="Index" asp-route-categoryId="@cat.CategoryId">
                    @cat.CategoryName
               </a> 
            </li>
        }
    </ul>
</div>